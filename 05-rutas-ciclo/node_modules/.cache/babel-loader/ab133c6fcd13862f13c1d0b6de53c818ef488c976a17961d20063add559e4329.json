{"ast":null,"code":"export default {\n  props: {\n    link: {\n      type: Object,\n      required: true\n    }\n  },\n  computed: {\n    isExternalLink() {\n      return this.link.to.startsWith('http');\n    }\n  }\n};","map":{"version":3,"names":["props","link","type","Object","required","computed","isExternalLink","to","startsWith"],"sources":["/home/des-sis18/proyectosHTML/VueJsCourse/BasesVue/vueApp/05-rutas-ciclo/src/modules/shared/components/CustomLink.vue"],"sourcesContent":["<template>\n    <a \n      v-if=\"isExternalLink\"\n      class=\"normal-link\"\n      target=\"__blank\"\n      :href=\"link.to\">{{ link.name }}</a>\n\n    <router-link \n      v-else\n      :to=\"{name:link.to, params:{id:link.id} }\"\n      v-slot=\"{isActive}\">\n        <a \n          :href=\"href\"\n          :class=\"isActive ? 'is-active' : 'normal-link'\"\n        >\n        {{ link.name }}\n        </a>\n      </router-link>\n</template>\n\n<script>\n\n    export default {\n      props:{\n        link:{\n          type:Object,\n          required:true\n\n        },\n      },\n      computed:{\n        isExternalLink(){\n          return this.link.to.startsWith('http')\n        }\n      }  \n    }\n</script>\n<style scoped>\n\n.is-active{\n  text-decoration: none;\n  color:#42b983;\n}\n\n.normal-link{\n  text-decoration: none;\n  color:#c6c5c5;\n}\n    \n</style>"],"mappings":"AAsBI,eAAe;EACbA,KAAK,EAAC;IACJC,IAAI,EAAC;MACHC,IAAI,EAACC,MAAM;MACXC,QAAQ,EAAC;IAEX;EACF,CAAC;EACDC,QAAQ,EAAC;IACPC,cAAcA,CAAA,EAAE;MACd,OAAO,IAAI,CAACL,IAAI,CAACM,EAAE,CAACC,UAAU,CAAC,MAAM;IACvC;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}